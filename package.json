{
  "name": "fox-framework",
  "version": "1.0.0",
  "description": "A modern web framework for TypeScript/Node.js with modular routing, integrated template engine, and factory patterns",
  "main": "dist/tsfox/index.js",
  "types": "dist/tsfox/index.d.ts",
  "bin": {
    "tsfox": "dist/tsfox/cli/index.js"
  },
  "scripts": {
    "start": "ts-node src/server/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --watch 'tsfox/**/*.ts' --exec 'ts-node' src/server/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.ts'",
    "test:unit": "jest --testMatch='**/__tests__/**/*.test.ts' --ignore-pattern='integration'",
    "clean": "rm -rf dist coverage",
    "lint": "echo 'ESLint not configured yet'",
    "lint:fix": "echo 'ESLint not configured yet'"
  },
  "keywords": [
    "framework",
    "web",
    "typescript",
    "nodejs",
    "express",
    "template-engine",
    "factory-pattern",
    "cli"
  ],
  "author": "Fox Framework Team",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.0.40",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "inquirer": "^9.3.7",
    "ora": "^7.0.1"
  }
}
